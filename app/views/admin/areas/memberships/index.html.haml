.columns
  = render :partial => 'admin/areas/nav'
  
  %h2 Members
  
  %table
    %tr
      %th Name
      %th Email
      
    - @memberships.each do |membership|
      %tr
        %td=link_to membership.user.full_name, edit_admin_user_path(membership.user,  :area_id =>  @area.id)
        %td= membership.user.email
